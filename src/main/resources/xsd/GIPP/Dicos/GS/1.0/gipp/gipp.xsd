			<xs:documentation>Time, seasonal or datation correction</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="X" type="gipp:A_COR_LOC_TIME_SEASONAL_DATATION_BLOCK" minOccurs="0"/>
