	<xs:complexType name="AN_ARRAY_OF_DOUBLE">
		<xs:annotation>
			<xs:documentation>Description of a 2D Array of 64bits Double elements</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="NB_OF_ROWS">
				<xs:annotation>
					<xs:documentation>Number of rows</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:minInclusive value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="NB_OF_COLS">
				<xs:annotation>
					<xs:documentation>Number of columns in the array.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:minInclusive value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ORIGIN">
				<xs:annotation>
					<xs:documentation>Array count start</xs:documentation>
					<xs:appinfo>
						<gs2:DOC>
							<gs2:DEFINITION>Array Origin can start from 0 or from 1. &lt;br>
								Array first element is found in (0,0) (if ORIGIN=0) or in (1,1) (ORIGIN=1)</gs2:DEFINITION>
							<gs2:INIT_VALUE>1</gs2:INIT_VALUE>
						</gs2:DOC>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:enumeration value="0"/>
						<xs:enumeration value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ARRAY">
				<xs:annotation>
					<xs:documentation>The array data storage.</xs:documentation>
					<xs:appinfo>
						<gs2:DOC>
							<gs2:DEFINITION>Data are stored row by row, there are NB_OF_ROWS rows, and NB_OF_COLS elements in each row.</gs2:DEFINITION>
						</gs2:DOC>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ROW" type="misc:A_LIST_OF_DOUBLE" maxOccurs="unbounded"/>
					</xs:sequence>
