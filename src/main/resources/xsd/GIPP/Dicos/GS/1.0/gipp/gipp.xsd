	<xs:complexType name="AN_UNIDIM_FILTER">
		<xs:annotation>
			<xs:documentation>1D filter definition </xs:documentation>
			<xs:appinfo>
				<gs2:DOC>
					<gs2:DEFINITION><![CDATA[An image filter is defined with :<br/>
					<ul>
					<li>its type (a textual enumeration);</li>
					<li>the position of filter center : this position has different meanings according to the file type :<br>
						 <ul>
						 <li>  spectral filter :position of the filter center </li>
						 <li>  spectral filter : position of the continue coef in the filter </li>
						  <li>  wavelet filter : position of the continue coef in the filter </li>
						  </ul>										
					</li>
					<li>2 oversampling factors (row wise, column wise) : usefull for geometrical transforms (resampling filters for example);</li>
					<li>a data representation : choice between </li>
						 <ul>
						 <li>  an array of double for a bidim filter definition </li>
						 <li>  2 lists of doubles representing the 2 1D-filters (row and column wise) if the image filter is separable  </li>
						 </ul>	

					</ul>]]></gs2:DEFINITION>
				</gs2:DOC>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="NB_OF_ROWS">
				<xs:annotation>
					<xs:documentation>Number of rows of the filter (=1 for a row wise 1D filter)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:minInclusive value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="NB_OF_COLS">
				<xs:annotation>
					<xs:documentation>Number of columns of the filter (=1 for a column wise 1D filter)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:minInclusive value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ORIGIN">
				<xs:annotation>
					<xs:documentation>Center of the filter</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:minInclusive value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="COEFS" type="misc:A_LIST_OF_DOUBLE">
				<xs:annotation>
					<xs:documentation>Coefficients for the 1D filter</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AN_IMAGE_RESAMPLING_FILTER">
		<xs:annotation>
			<xs:documentation>Resampling filter definition</xs:documentation>
			<xs:appinfo>
				<gs2:DOC>
					<gs2:DEFINITION>The Resampling filter is defined by a filter name (bicubic, bilinear) knowned by LAI treatments, or by the description of a spatial filter. THE FILTER_DESCRIPTION/FILTER_TYPE should be "SPATIAL_FILTER"</gs2:DEFINITION>
				</gs2:DOC>
			</xs:appinfo>
		</xs:annotation>
		<xs:choice>
			<xs:element name="INTERNAL_FILTER">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="FILTER_NAME" type="gipp:AN_INTERNAL_FILTER_NAME">
							<xs:annotation>
								<xs:documentation>List of internal filter name</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="MODE" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Filter application mode (default = EXACT)</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="EXACT"/>
									<xs:enumeration value="PRECALCULE"/>
									<xs:enumeration value="TABULE"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="PRECISION" type="xs:positiveInteger" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Computation accuracy if MODE=PRECALCULE</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="EXTERNAL_FILTER" type="gipp:AN_IMAGE_FILTER"/>
		</xs:choice>
	</xs:complexType>
