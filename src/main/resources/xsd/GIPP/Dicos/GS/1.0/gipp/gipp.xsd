	<xs:complexType name="A_COR_LOC_TIME_SEASONAL_DATATION_BLOCK">
		<xs:sequence>
			<xs:element name="NB_DATE_TIME">
				<xs:annotation>
					<xs:documentation>Number of applicability dates time for the correction model</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DATE_TIME_LIST">
				<xs:annotation>
					<xs:documentation>List of the NB_DATE_TIME applicable dates time</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="DATE_TIME" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Time interval = [begin date time, end date time [</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="BEGIN" type="date_time:A_TAI_DATE_TIME">
										<xs:annotation>
											<xs:documentation>Begin date time</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="END" type="date_time:A_TAI_DATE_TIME">
										<xs:annotation>
											<xs:documentation>End date time</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="JULIAN_DAY" type="date_time:A_JULIAN_DAY">
										<xs:annotation>
											<xs:documentation>Reference Julian Day</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="COR_MODEL_VALUES" type="misc:A_LIST_OF_DOUBLE">
										<xs:annotation>
											<xs:documentation>List of the model correction values. If correction_model is in{TIME_POLYNOMIAL,  DATATION}, then values = coeff0 coeff1 coeff2 ...  coeffN.  If correction_model is SEASONAL, then values = Amplitude Omega Phase.  If correction_model is TIME_POLYSINUS, then values = Amplitude Omega Phase coeff0 coeff1 coeff2 ... coeffN. </xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
