										</xs:simpleType>
									</xs:element>
								</xs:sequence>
								<xs:attribute name="band_id" type="image:A_BAND_NUMBER" use="required"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="SPATIAL_MARGIN">
				<xs:annotation>
					<xs:documentation>Spatial margin defined per band to take care of the deconvolution effect</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="BAND" maxOccurs="13">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="MARGIN">
										<xs:simpleType>
											<xs:restriction base="xs:int">
												<xs:minInclusive value="0"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
								</xs:sequence>
								<xs:attribute name="band_id" type="image:A_BAND_NUMBER" use="required"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
