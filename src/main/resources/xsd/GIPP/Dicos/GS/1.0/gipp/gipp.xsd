				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="RESTORATION_PARAMETERS" maxOccurs="13">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="RESTORATION_SCENARIO" type="image:A_RESTORATION_SCENARIO">
										<xs:annotation>
											<xs:documentation>Type of restoration processing</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="LEVELLING_VALUES" type="gipp:A_RESTORATION_LEVELLING_VALUES">
										<xs:annotation>
											<xs:documentation>Parameters for levelling : minimum and maximum value</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="F_CONVOLUTION_PARAMETERS" type="gipp:AN_ASTRID_F_CONVOLUTION_PARAMETERS">
										<xs:annotation>
											<xs:documentation>Isolated parameters for convolution</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="W_DENOISING_PARAMETERS" type="gipp:AN_ASTRID_W_DENOISING_PARAMETERS">
										<xs:annotation>
											<xs:documentation>Isolated parameters for denoising</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
								<xs:attribute name="band_id" type="image:A_BAND_NUMBER" use="required"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
				<xs:unique name="band_id_restoration_unique">
					<xs:selector xpath="./RESTORATION_PARAMETERS"/>
					<xs:field xpath="@band_id"/>
				</xs:unique>
			</xs:element>
			<xs:element name="PIXELS_NO_DATA_PARAMETERS">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="BAND" maxOccurs="13">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="INTERPOLATION_FILTER" type="gipp:AN_IMAGE_RESAMPLING_FILTER">
										<xs:annotation>
											<xs:documentation>Interpolation filter used for the management of NO_DATA pixels</xs:documentation>
										</xs:annotation>
