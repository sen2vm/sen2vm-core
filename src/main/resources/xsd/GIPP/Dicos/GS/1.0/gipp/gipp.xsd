	<xs:simpleType name="AN_IMAGE_FILTER_TYPE">
		<xs:annotation>
			<xs:documentation>Image filter type</xs:documentation>
			<xs:appinfo>
				<gs2:DOC>
					<gs2:DEFINITION>Textual enumeration of image filter types</gs2:DEFINITION>
				</gs2:DOC>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="SPATIAL"/>
			<xs:enumeration value="FREQUENTIAL"/>
			<xs:enumeration value="WAVELET"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="AN_IMAGE_FILTER">
		<xs:annotation>
			<xs:documentation>Image filter definition</xs:documentation>
			<xs:appinfo>
				<gs2:DOC>
					<gs2:DEFINITION><![CDATA[An image filter is defined with :<br/>
					<ul>
					<li>its type (a textual enumeration);</li>
					<li>the position of filter center : this position has different meanings according to the file type :<br>
						 <ul>
						 <li>  spectral filter :position of the filter center </li>
						 <li>  spectral filter : position of the continue coef in the filter </li>
						  <li>  wavelet filter : position of the continue coef in the filter </li>
						  </ul>										
					</li>
					<li>2 oversampling factors (row wise, column wise) : usefull for geometrical transforms (resampling filters for example);</li>
					<li>a data representation : choice between </li>
						 <ul>
						 <li>  an array of double for a bidim filter definition </li>
						 <li>  2 lists of doubles representing the 2 1D-filters (row and column wise) if the image filter is separable  </li>
						 </ul>	

					</ul>]]></gs2:DEFINITION>
				</gs2:DOC>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FILTER_TYPE" type="gipp:AN_IMAGE_FILTER_TYPE"/>
			<xs:element name="FILTER_CENTER">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ROW_INDEX" type="xs:positiveInteger"/>
						<xs:element name="COL_INDEX" type="xs:positiveInteger"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="FILTER_OVERSAMPLING">
				<xs:annotation>
					<xs:documentation>Oversampling between Filter and Image</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ROW_WISE" type="xs:int">
							<xs:annotation>
								<xs:documentation>Filter tabulated at 1/ROW_WISE image pixels along the rows</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="COL_WISE" type="xs:int">
							<xs:annotation>
								<xs:documentation>Filter tabulated at 1/COL_WISE image pixels along the col</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="FILTER_DATA">
				<xs:complexType>
					<xs:choice>
						<xs:element name="BIDIM_FILTER_DATA" type="gipp:AN_ARRAY_OF_DOUBLE"/>
						<xs:element name="SEPARABLE_FILTER_DATA">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="NB_OF_ROWS" type="xs:int"/>
									<xs:element name="NB_OF_COLS" type="xs:int"/>
									<xs:element name="ORIGIN" type="xs:int">
										<xs:annotation>
											<xs:documentation>array count start</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="ROW_COEFS" type="misc:A_LIST_OF_DOUBLE">
										<xs:annotation>
											<xs:documentation>coefficients for the row wise 1D filter</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="COL_COEFS" type="misc:A_LIST_OF_DOUBLE">
										<xs:annotation>
											<xs:documentation>coefficients for the column wise 1D filter</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:choice>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="AN_INTERNAL_FILTER_NAME">
		<xs:annotation>
			<xs:documentation>The name of an internal filter</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="BCO"/>
			<xs:enumeration value="BLN"/>
			<xs:enumeration value="PPV"/>
