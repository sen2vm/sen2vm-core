												<xs:element name="DETECTOR_LIST">
													<xs:complexType>
														<xs:sequence>
															<xs:element name="DETECTOR" maxOccurs="12">
																<xs:complexType>
																	<xs:sequence>
																		<xs:element name="BEGIN_NB_LINES_TO_CUT">
																			<xs:annotation>
																				<xs:documentation>Number of lines to cut at the beginning of the segment</xs:documentation>
																			</xs:annotation>
																			<xs:simpleType>
																				<xs:restriction base="xs:int">
